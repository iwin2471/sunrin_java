import java.io.FileInputStream;
import java.io.FileOutputStream;

public class ex1 {
	public static void main(String[] args) {
		FileInputStream fi = new FileInputStream("a.jpg");
		FileOutputStream fo = new FileOutputStream("b.jpg");
		int inp;
		while(true){
			inp = fi.read();
			fo.write(inp);
		}
	}
}
